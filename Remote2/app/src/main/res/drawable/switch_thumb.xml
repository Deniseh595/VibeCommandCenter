<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <layer-list>
            <item
                android:top="1dp"
                android:end="1dp"
                android:width="28dp"
                android:height="28dp">
                <shape android:shape = "oval">
                    <solid android:color = "@color/thumbon"/>
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_checked="false">
        <layer-list>
            <item
                android:top="1dp"
                android:left="1dp"
                android:width="28dp"
                android:height="28dp">
                <shape android:shape = "oval">
                    <solid android:color = "@color/font_color" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item >
        <layer-list>
            <item
                android:top="1dp"
                android:left="1dp"
                android:width = "28dp"
                android:height = "28dp">
                <shape android:shape = "oval">
                    <solid android:color = "@color/switchtrackdisable" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>
